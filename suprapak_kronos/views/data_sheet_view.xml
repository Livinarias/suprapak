<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="form_data_sheet" model="ir.ui.view">
            <field name="name">form.data.sheet</field>
            <field name="model">data.sheet</field>
            <field name="arch" type="xml">
                <form string="Data sheet">
                    <header>
                        <button string="New Quotation" name="action_sale_quotations_new" type="object" class="oe_highlight"/>
                        <button class="oe_highlight" type="object" name="action_bom_line" string="Create Versions"/>
                        <field name="state" widget="statusbar" clickable="True"/>
                    </header>
                    <header>
                        <button string="Review" class="btn btn-primary" type="object" name="progressbar_review" />
                            <button string="Technical Approval" class="btn btn-warning" type="object" name="progressbar_technical" />
                            <button string="Design approval" class="btn btn-secondary" type="object" name="progressbar_design" />
                            <button string="Approved" class="btn btn-success" type="object" name="progressbar_approved" />
                            <button string="Rejected" class="btn btn-danger" type="object" name="progressbar_rejected" />
                            <button string="Obsolete" class="btn btn-info" type="object" name="progressbar_obsolete" />
                            <button string="Rejected Technical" class="btn btn-light" type="object" name="progressbar_rejected" />
                            <button string="Rejected Design" type="object" name="progressbar_rejected_d" />
                    </header>
                    <sheet>
                        <div class="oe_button_box" name="button_box">
                            <button class="oe_stat_button" type="object" name="action_view_sale_quotation" icon="fa-pencil-square-o">
                                <field name="quotation_count" widget="statinfo" string="Quotations"/>
                            </button>
                        </div>
                            <div class="oe_title">
                                <label for="name" class="oe_edit_only" string="Name"/>
                                <h1>
                                    <field name="name" required="True"/>
                                </h1>
                            </div>
                            <div class="oe_title">
                                <label for="name" class="oe_edit_only" string="VersiÃ³n"/>
                                <h1>
                                    <field name="version" readonly="True"/>
                                </h1>
                            <!-- Montaje multiple -->
                            </div>
                        <separator string="General" colspan="2"/>
                        <group>
                            <group>
                                <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" required="True" domain="[('tipo_producto','=','terminado')]"/>
                                <field name="opportunity_id" options="{'no_create': True, 'no_create_edit':True}" readonly="True"/>
                            </group>
                            <group>
                                <field name="priority" widget="priority"/>
                                <field name="type_sheet"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Basic information">
                                <group>
                                    <group>
                                        <field name="partner_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="commentary"/>
                                        <field name="product_code"/>
                                        <field name="sector_id"/>
                                    </group>
                                    <group>
                                        <field name="team_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="currency_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                    </group>
                                    <group string="customer packaging">
                                        <field name="tag_form_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="tag_color_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="material_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="application_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="position_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="content_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                    </group>
                                    <group>
                                        <field name="photo" widget="pdf_viewer"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Technical information">
                                <group>
                                    <group>
                                        <field name="print" widget="checkbox"/>
                                        <field name="no_print" widget="checkbox"/>
                                        <field name="print_class" widget="radio" attrs="{'invisible':[('print', '!=', True)]}"/>
                                        <field name="print_type_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('print', '!=', True)]}"/>
                                        <field name="required_match_print" widget="checkbox"/>
                                        <field name="product_type_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="chemical_composition" options="{'no_create': True, 'no_create_edit':True}"/>
                                    </group>
                                    <!--<group>-->
                                        <group>
                                            <field name="quantity"/>
                                            <field name="uom_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                            <field name="form_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                            <field name="movie_type_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                            <field name="transversal" />
                                            <field name="longitudinal" />
                                            <field name="movie_type_product_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                        </group>
                                    <!--</group>-->
                                    <group>
                                        <field name="specification_width_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="specification_long_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="caliber_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="overlap_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="overlap_location_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="rhombus" attrs="{'invisible':[('overlap_id', '!=', True)]}"/>
                                    </group>
                                    <group>
                                        <field name="tolerance_width" readonly="True"/>
                                        <field name="tolerance_long" readonly="True"/>
                                        <field name="tolerance_caliber" readonly="True"/>
                                        <field name="tolerance_overlap" readonly="True"/>
                                        <field name="rhombus"/>
                                        <h1><br></br></h1>
                                        <field name="average_label_weight" attrs="{'invisible':[('presentation','=','ROLLO')]}"/>
                                        <field name="roll_weight" attrs="{'invisible':[('presentation','!=','ROLLO')]}"/>
                                    </group>
                                    <group>
                                        <!--rollo-->
                                        <field name="presentation_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="presentation" invisible="True"/>
                                        <field name="meter_per_roll" attrs="{'invisible':[('presentation','!=','ROLLO')]}"/>
                                        <field name="core_diameter_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('presentation','!=','ROLLO')]}"/>
                                        <field name="embossing_number_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('presentation','!=','ROLLO')]}"/>
                                        <field name="number_splices_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('presentation','!=','ROLLO')]}"/>
                                        <fields name="splice_type_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('presentation','!=','ROLLO')]}"/>
                                        <field name="splicing_tape_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('presentation','!=','ROLLO')]}"/>
                                    </group>
                                    <group>
                                        <!--sellado-->
                                        <field name="seal_type_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('presentation', '!=','SELLADO')]}"/>
                                        <field name="sealing_tab" attrs="{'invisible':[('presentation', '!=','SELLADO')]}"/>
                                        <field name="seal" invisible="1"/>
                                    </group>
                                    <group>
                                        <!--preformado-->
                                        <field name="mold_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('presentation', '!=','PREFORMADO')]}"/>
                                        <field name="bottom_diameter" attrs="{'invisible':[('presentation', '!=','PREFORMADO')]}"/>
                                        <field name="upper_diameter" attrs="{'invisible':[('presentation', '!=','PREFORMADO')]}"/>
                                        <field name="tab_length" attrs="{'invisible':[('presentation', '!=','PREFORMADO')]}"/>
                                        <field name="band_height" attrs="{'invisible':[('presentation', '!=','PREFORMADO')]}"/>
                                        <field name="product" attrs="{'invisible':[('presentation', '!=','PREFORMADO')]}"/>
                                        <field name="long_modification" attrs="{'invisible':[('presentation', '!=','PREFORMADO')]}"/>
                                    </group>
                                    <group>
                                        <!--plano mecanico-->
                                        <field name="mechanic_plan_ids">
                                            <tree>
                                                <field name="name" widget="pdf_viewer"/>
                                            </tree>
                                        </field>
                                        <!-- Montaje multiple -->
                                        <field name="multiple_mount" widget="checkbox"/>
                                        <field name="sheet_ids" context="{'default_repeat_id': repeat_id, 'default_id_char': id}" attrs="{'invisible':[('multiple_mount', '!=', True)]}">
                                            <tree editable="bottom">
                                                <field name="repeat_id" invisible="True"/>
                                                <field name="id_char" invisible="True"/>
                                                <field name="id_sheet" domain="[('repeat_id','=',repeat_id),('id','!=',id_char)]"/>
                                                <field name="repetition" required="True" />
                                                <field name="description"/>
                                            </tree>
                                        </field>
                                    </group>
                                    <group>
                                        <field name="barcode_type_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="barcode_number"/>
                                    </group>
                                    <group>
                                        <field name="microperforated" widget="checkbox"/>
                                        <field name="microperforated_ids" attrs="{'invisible':[('microperforated', '!=', True)]}">
                                            <tree>
                                                <field name="cross"/>
                                                <field name="logitudinal"/>
                                            </tree>
                                        </field>
                                        <field name="guillotine" widget="checkbox"/>
                                        <field name="guillotine_mm" attrs="{'invisible':[('guillotine', '!=', True)]}"/>
                                    </group>
                                    <group>
                                        <field name="drawn" widget="checkbox"/>
                                        <field name="drawn_type_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('drawn', '!=', True)]}"/>
                                        <field name="drawn_presentation_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('drawn', '!=', True)]}"/>
                                        <field name="tongue" attrs="{'invisible':[('drawn', '!=', True)]}"/>
                                    </group>
                                    <group>
                                        <field name="waistband" widget="checkbox"/>
                                        <field name="color_movie_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('waistband', '!=', True)]}"/>
                                    </group>
                                    <group>
                                        <field name="thermal_adhesive" widget="checkbox"/>
                                        <field name="adhesive_type_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('thermal_adhesive', '!=', True )]}"/>
                                        <field name="adhesive_type_selector" invisible="1"/>
                                        <field name="rod_number" attrs="{'invisible':[('adhesive_type_selector', '!=','EN BARRA')]}"/>
                                    </group>
                                    <group>
                                        <field name="photo_format" widget="pdf_viewer" attrs="{'invisible':[('adhesive_type_selector', '!=','IMPRESO')]}"/>
                                    </group>
                                    <group string="Acabados Especiales">
                                        <field name="cold_foil" widget="checkbox"/>
                                        <field name="cold_foil_id" options="{'no_create': True, 'no_create_edit':True}" attrs="{'invisible':[('cold_foil', '!=',True)]}"/>
                                        <field name="cold_foil_selector" invisible="1"/>
                                        <field name="cold_foil_width" attrs="{'invisible':[('cold_foil_selector', '!=','PLATA')]}"/>
                                        <field name="cast" widget="checkbox"/>
                                        <field name="cast_reference" attrs="{'invisible':[('cast', '!=',True)]}"/>
                                        <field name="cast_width" attrs="{'invisible':[('cast', '!=',True)]}"/>
                                    </group>
                                    <group string="Tintas Especiales">
                                        <field name="ink_ids">
                                            <tree>
                                                <field name="name"/>
                                                <field name="percentage"/>
                                            </tree>
                                        </field>
                                    </group>
                                </group>
                            </page>
                            <page string="Design Features">
                                <group>
                                    <group>
                                        <field name="required_match_print" widget="checkbox"/>
                                        <field name="match_print_ids" attrs="{'invisible':[('required_match_print', '!=',True)]}" widget="one2many">
                                            <tree>
                                                <field name="datetime"/>
                                                <field name="quantity"/>
                                                <field name="customer"/>
                                                <field name="match_print_id"/>
                                                <field name="sign_vendor"/>
                                                <field name="deliver_to"/>
                                                <field name="sign_designer"/>
                                                <field name="vendor_date"/>
                                                <field name="date_recieved_approved"/>
                                                <field name="observations"/>
                                            </tree>
                                        </field>
                                    </group>
                                    <group>
                                        <field name="designer" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="repeat_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('large_planned','=',specification_long_id)]"/>
                                        <field name="room_large"/>
                                        <field name="large_planned"/>
                                        <field name="specification_width_planned"/>
                                    </group>
                                    <group>
                                        <field name="color_scale_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                        <field name="complexity"/>
                                    </group>
                                    <group>
                                        <field name="prints_ids">
                                            <tree>
                                                <field name="name" options="{'no_create': True, 'no_create_edit':True}"/>
                                                <field name="press"/>
                                                <field name="line"/>
                                                <field name="lineatura"/>
                                                <field name="bcm"/>
                                            </tree>
                                        </field>
                                    </group>
                                    <group>
                                        <field name="control_changes_ids">
                                            <tree>
                                                <field name="name"/>
                                                <field name="date"/>
                                                <field name="vendor"/>
                                            </tree>
                                        </field>
                                        <field name="change_observation"/>
                                    </group>
                                    <group>
                                        <field name="plane_art" widget="pdf_viewer"/>
                                    </group>
                                    <group>
                                        <field name="funtional_test" widget="pdf_viewer"/>
                                    </group>
                                </group>
                            </page>
                            <page string="packaging characteristics">
                                    <group>
                                        <group string="packing">
                                            <field name="separator_id" attrs="{'invisible':[('presentation','!=','ROLLO')]}" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                            <field name="core_diameter" attrs="{'invisible':[('presentation','!=','ROLLO')]}" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                            <field name="width_core" attrs="{'invisible':[('presentation','!=','ROLLO')]}" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                            <field name="bag" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                            <field name="box" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                            <field name="superlon" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                        </group>
                                        <group string="lashing straps">
                                            <field name="roll_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                            <field name="for_bag_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                            <field name="for_superlon_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                            <field name="for_box"/>
                                        </group>
                                        <group string="Cintas de empalme por proceso ">
                                            <field name="refile_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                            <field name="revision_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                            <field name="gluped_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                            <field name="gluped2_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                            <field name="rebobine_ids" widget="one2many" context="{'default_sheet_id': id}">
                                                <tree string="Components" editable="bottom">
                                                    <field name="product_id" options="{'no_create': True, 'no_create_edit':True}" domain="[('tipo_producto','=','materia')]"/>
                                                </tree>
                                            </field>
                                        </group>
                                    </group>
                            </page>
                            <page string="Bills of materials">
                                <group>
                                    <field name="line_ids">
                                        <tree editable="bottom">
                                            <field name="product_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                            <field name="product_qty"/>
                                            <field name="uom_categ_id" invisible="True"/>
                                            <field name="uom_id"/>
                                        </tree>
                                    </field>
                                </group>
                                <group>
                                    <field name="routing_id" options="{'no_create': True, 'no_create_edit':True}"/>
                                </group>
                            </page>
                            <page string="Versions">
                                <group>
                                    <field name="bom_id" options="{'no_create': True, 'no_create_edit':True}" readonly="True"/>
                                </group>
                                <group>
                                    <field name="bom_ids">
                                        <tree>
                                            <field name="product_tmpl_id"/>
                                            <field name="product_qty"/>
                                            <field name="product_uom_id"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                            <page string="Production orders">
                                <group>
                                    <field name="production_ids" readonly="True"/>
                                </group>
                                <group>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread" options="{'post_refresh': 'recipients'}"/>
                    </div>
                </form>

            </field>
        </record>
        <record id="tree_data_sheet" model="ir.ui.view">
            <field name="name">tree.data.sheet</field>
            <field name="model">data.sheet</field>
            <field name="arch" type="xml">
                <tree string="Data sheet">
                    <field name="name"/>
                    <field name="product_id"/>
                    <field name="partner_id"/>
                    <field name="priority"/>
                    <field name="type_sheet" readonly="True"/>
                    <field name="version"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_data_sheet">
            <field name="name">Data sheet</field>
            <field name="res_model">data.sheet</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="action_new_data_sheet_new" model="ir.actions.act_window">
            <field name="name">Data sheet</field>
            <field name="res_model">data.sheet</field>
            <field name="view_mode">form,tree</field>
            <field name="domain">[('opportunity_id', '=', active_id)]</field>
            <field name="context">{'search_default_opportunity_id': active_id, 'default_opportunity_id': active_id}
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_data_product_type">
            <field name="name">Product type</field>
            <field name="res_model">data.product.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_drawn_type">
            <field name="name">Drawn type</field>
            <field name="res_model">data.drawn.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_movie_type">
            <field name="name">Movie type</field>
            <field name="res_model">data.movie.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_movie_color">
            <field name="name">Colors</field>
            <field name="res_model">data.movie.color</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_caliber_type">
            <field name="name">Caliber type</field>
            <field name="res_model">data.caliber.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_tag_form">
            <field name="name">Form</field>
            <field name="res_model">data.tag.form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_material">
            <field name="name">material</field>
            <field name="res_model">data.material</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_application_mode">
            <field name="name">material</field>
            <field name="res_model">data.application.mode</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_application_position">
            <field name="name">material</field>
            <field name="res_model">data.application.position</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_print_type">
            <field name="name">Print Type</field>
            <field name="res_model">print.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_material">
            <field name="name">Data material</field>
            <field name="res_model">data.material</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.actions.act_window" id="action_data_application_mode">
            <field name="name">Data Application Mode</field>
            <field name="res_model">data.application.mode</field>
            <field name="view_mode">tree,form</field>
        </record>
